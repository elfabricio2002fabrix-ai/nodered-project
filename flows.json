    {
        "id": "control_motor_node",
        "type": "function",
        "z": "d3d72eb37a8693c8",
        "name": "L贸gica Control Motor",
        "func": "// Obtener el contexto para mantener el estado\nlet estadoMotor = context.get('estadoMotor') || {\n    encendido: false,\n    velocidad: 0\n};\n\n// Procesar el comando recibido\nlet comando = msg.payload.comando;\nlet mensaje = \"\";\n\nswitch(comando) {\n    case 'encender':\n        estadoMotor.encendido = true;\n        if (estadoMotor.velocidad === 0) {\n            estadoMotor.velocidad = 50; // Velocidad por defecto\n        }\n        mensaje = \"Motor ENCENDIDO - Velocidad: \" + estadoMotor.velocidad + \"%\";\n        break;\n        \n    case 'apagar':\n        estadoMotor.encendido = false;\n        mensaje = \"Motor APAGADO\";\n        break;\n        \n    case 'velocidad':\n        if (estadoMotor.encendido) {\n            estadoMotor.velocidad = msg.payload.valor;\n            mensaje = \"Velocidad ajustada a: \" + estadoMotor.velocidad + \"%\";\n        } else {\n            mensaje = \"锔 El motor est谩 apagado. Enci茅ndalo primero.\";\n        }\n        break;\n        \n    default:\n        mensaje = \"Comando no reconocido\";\n}\n\n// Guardar el estado actualizado\ncontext.set('estadoMotor', estadoMotor);\n\n// Preparar mensaje de salida para informaci贸n\nmsg.payload = mensaje;\nmsg.estadoMotor = estadoMotor;\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 470,
        "y": 660,
        "wires": [
            [
                "ui_text_info",
                "salida_mqtt"
            ]
        ]
    },
